{
  "name": "Liker by rohanchaubey.com",
  "version": "1.01",
  "author": "Vadagon",
  "manifest_version": 2,
  "description": "Automate your social media",
  "permissions": [
    "tabCapture",
    "activeTab",
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "Click to open popup",
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
      "scripts": ["scripts/background.js"]
  },
  "content_scripts": [{
      "matches": ["*://*.twitter.com/*", "*://*.facebook.com/*", "*://*.linkedin.com/*", "*://*.instagram.com/*"],
      "js": ["bower_components/jquery/dist/jquery.min.js", "scripts/content.js"]
  }],
  "options_page": "options.html"
}