{
  "name": "Follower Potato",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "description",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "options_page": "src/options/index.html",
  "permissions": [
    "https://*/*",
    "http://*/*",
    "background",
    "storage"
  ],
  "browser_action": {
    "default_title": "FollowerPotato"
  },
  "content_scripts": [
    {
      "matches": ["http://www.instagram.com/*", "https://www.instagram.com/*"],
      "js": ["js/content.js"],
      "css": ["css/content.css"]
    }
  ],
  "web_accessible_resources": ["/*"]
}